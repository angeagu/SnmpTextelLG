				 5 - 600 (seg)"
			::= { consultaInfoConfig 4 }

		refrescoSx OBJECT-TYPE
			SYNTAX Integer32
			ACCESS read-only
			STATUS current
			DESCRIPTION
				"Tiempo entre llamadas SX
				 5 - 600 (seg)"
			::= { consultaInfoConfig 5 }

		alternanciaMultiidioma OBJECT-TYPE
			SYNTAX Integer32
