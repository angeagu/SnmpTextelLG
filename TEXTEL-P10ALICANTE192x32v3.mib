			masterResetSWNA OBJECT-TYPE
				SYNTAX Integer32
				ACCESS read-write
				STATUS current
				DESCRIPTION
					"Reset Software Sin Apagar (1 = SI)."
				::= { masterErrores 12 }

			masterResetHW OBJECT-TYPE
				SYNTAX Integer32
				ACCESS read-write
				STATUS current
				DESCRIPTION
					"Reset Hardware (1 = SI)."
				::= { masterErrores 13 }

