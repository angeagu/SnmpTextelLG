				::= { masterErrores 4 }
	
			masterTermostato OBJECT-TYPE
				SYNTAX Integer32
				ACCESS read-write
				STATUS current
				DESCRIPTION
					"Estado termostato (0 = OFF / 1 = ON)."
				::= { masterErrores 5 }
	
			masterVentilador1 OBJECT-TYPE
				SYNTAX OCTET STRING
				ACCESS read-only
				STATUS current
				DESCRIPTION
					"Estado del ventilador 1 (Ok / Error)."
				::= { masterErrores 6 }
	
			masterVentilador2 OBJECT-TYPE
				SYNTAX OCTET STRING
				ACCESS read-only
				STATUS current
				DESCRIPTION
