TEXTEL-P10ALICANTE192x32v3 DEFINITIONS ::= BEGIN


	IMPORTS
	NOTIFICATION-TYPE, OBJECT-TYPE, MODULE-IDENTITY, 
	enterprises, Integer32
		FROM SNMPv2-SMI
	NOTIFICATION-GROUP, OBJECT-GROUP, MODULE-COMPLIANCE
		FROM SNMPv2-CONF
	RowStatus, DisplayString
		FROM SNMPv2-TC;


textel-Marimon 	OBJECT IDENTIFIER ::= { enterprises 46536 }


	nombreCompleto OBJECT-TYPE
	SYNTAX OCTET STRING
	ACCESS read-only
	STATUS current
	DESCRIPTION
		"Nombre del fabricante del display."
	::= { textel-Marimon 1 }


display OBJECT IDENTIFIER ::= { textel-Marimon 2 }

		
	numeroSerie OBJECT-TYPE
		SYNTAX OCTET STRING
		ACCESS read-only
		STATUS current
		DESCRIPTION
			"Nœmero de serie del fabricante"
		::= { display 1 }

	identificador OBJECT-TYPE
		SYNTAX OCTET STRING
		ACCESS read-only
		STATUS current
		DESCRIPTION
			"identificador del teleindicador."
		::= { display 2 }

	direccion OBJECT-TYPE
		SYNTAX OCTET STRING
		ACCESS read-only
		STATUS current
		DESCRIPTION
			"calle y numero."
		::= { display 3 }

	displayGeneralStatus OBJECT-TYPE
		SYNTAX OCTET STRING
		ACCESS read-only
		STATUS current
		DESCRIPTION
			"Estado general del teleindicador (Ok / Error)."
		::= { display 4 }


