				::= { masterNotificaciones 9 }  

	
trapParameters OBJECT IDENTIFIER ::= { textel-Marimon 3 }

		
	trapIp OBJECT-TYPE
		SYNTAX OCTET STRING
		ACCESS read-write
		STATUS current
		DESCRIPTION
			"IP del administrador para envio de traps."
		::= { trapParameters 1 }

	trapPort OBJECT-TYPE
		SYNTAX OCTET STRING
		ACCESS read-write
		STATUS current
		DESCRIPTION
			"Puerto del administrador para envio de traps."
		::= { trapParameters 2 }


configService OBJECT IDENTIFIER ::= { textel-Marimon 4 }


	initialConfig OBJECT IDENTIFIER ::= { configService 1 }
		
		idPanel OBJECT-TYPE
			SYNTAX Integer32
			ACCESS read-only
			STATUS current
			DESCRIPTION
				"identificador interno del operador
				 (0 - 9999999999)"
			::= { initialConfig 1 }

		modo OBJECT-TYPE
			SYNTAX Integer32
			ACCESS read-only
			STATUS current
			DESCRIPTION
				"modo en que trabaja el panel (
				 0 = deshabilitado,
				 1 = normal
				 2 = sin estimaciones
				 3 = mantenimiento)"
			::= { initialConfig 2 }

		modoMultiidioma OBJECT-TYPE
			SYNTAX Integer32
			ACCESS read-only
			STATUS current
			DESCRIPTION
				"multiidioma habilitado (
				 0 = no
				 1 = si)"
			::= { initialConfig 3 }

		idiomas OBJECT-TYPE
			SYNTAX OCTET STRING
			ACCESS read-only
			STATUS current
			DESCRIPTION
				"array de identificadores de idiomas,
				 uno como minimo (ES)"
			::= { initialConfig 4 }

	consultaInfoConfig OBJECT IDENTIFIER ::= { configService 2 }
		

		paradas OBJECT-TYPE
			SYNTAX OCTET STRING
			ACCESS read-only
			STATUS current
			DESCRIPTION
				"listado paradas para el ambito del panel."
			::= { consultaInfoConfig 1 }

		urlSm OBJECT-TYPE
			SYNTAX OCTET STRING
			ACCESS read-only
			STATUS current
			DESCRIPTION
				"Endpoint del servicio StopMonitoring."
