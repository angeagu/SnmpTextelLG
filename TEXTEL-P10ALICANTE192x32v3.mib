			masterImpacto NOTIFICATION-TYPE
				STATUS current
				DESCRIPTION
					"Impacto (en Gs)."
				::= { masterNotificaciones 8 }  

			masterErrorFotocelula NOTIFICATION-TYPE
				STATUS current
				DESCRIPTION
