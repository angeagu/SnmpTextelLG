trapParameters OBJECT IDENTIFIER ::= { textel-Marimon 3 }

		
	trapIp OBJECT-TYPE
		SYNTAX OCTET STRING
		ACCESS read-write
		STATUS current
		DESCRIPTION
			"IP del administrador para envío de traps."
		::= { trapParameters 1 }

	trapPort OBJECT-TYPE
		SYNTAX OCTET STRING
		ACCESS read-write
		STATUS current
		DESCRIPTION
			"Puerto del administrador para envío de traps."
