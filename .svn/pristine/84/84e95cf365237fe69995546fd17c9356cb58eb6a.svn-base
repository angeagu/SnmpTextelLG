package configuration.data;

import java.util.HashMap;

/**
 *
 * @author Tecnovision
 */
public class CommandForHW extends Command {

    private final String faceId;
    private final String cardId;
    private final HashMap<String, String> commandByLeafNodeValue;

    public CommandForHW(String faceId, String cardId, String type) {
        super(type);
        this.faceId = faceId;
        this.cardId = cardId;
        this.commandByLeafNodeValue = new HashMap<>();
    }
    
    public void addCommand(String value, String commandId){
        commandByLeafNodeValue.put(value, commandId);
    }

    public String getFaceId() {
        return faceId;
    }

    public String getCardId() {
        return cardId;
    }

    public String getCommandByLeafNodeValue(String value) {
        return commandByLeafNodeValue.get(value);
    }
}
