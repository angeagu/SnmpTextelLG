<CONFIG>
	<PANEL MODULE_WIDTH="32" MODULE_HEIGHT="16" COLUMNS="6" ROWS="3">
		<FACE ID="master">
			<CARD ID="AL1">
				<ON-START-COMMAND ID="DE" DELAY="15"/>
				<QUERY ID="D?" PERIOD="5" remarks="DISPLAY ON/OFF"/>
				<QUERY ID="P?" PERIOD="5" remarks="DOOR OPEN/CLOSED"/>
				<QUERY ID="T?" PERIOD="5" remarks="INTERNAL_TEMPERATURE"/>
				<QUERY ID="S?" PERIOD="5" remarks="THERMOSTAT ON/OFF"/>
				<QUERY ID="V?" PERIOD="5" remarks="FANS ON/OFF"/>
				<SW-QUERY ELEMENT_IDS="usb" remarks="ALARMS_CARD_USB_COMMPORT"/>
				<HW-NOTIFICATION NOTIFICATION_FORMAT="Ps" MESSAGE="Door Open"/>
				<HW-NOTIFICATION NOTIFICATION_FORMAT="Gxx" MESSAGE="Hit received with a G-Force of "/>
				<HW-NOTIFICATION NOTIFICATION_FORMAT="Ds" MESSAGE="Display Off"/>
				<SW-NOTIFICATION ELEMENT="masterUsbAlarmsNotification" CONDITION="STRING_VALUE_ERROR_CONDITION"/>
				<SW-NOTIFICATION ELEMENT="masterUsbErrorsNotification" CONDITION="STRING_VALUE_ERROR_CONDITION"/>
				<SW-NOTIFICATION ELEMENT="masterTemperatureNotification" CONDITION="BOUND" UPPER_BOUND="35"/>
				<SW-NOTIFICATION ELEMENT="masterFansNotification" CONDITION="VALUE_MUST_MATCH_TO" RETRIES="3"/>
				<COMMAND_ON_SNMP_SET OID="1.3.6.1.4.1.46536.2.6.1.2.0">
					<COMMAND LEAF_NODE_VALUE="0" ID="DA" remarks="APAGAR DISPLAY"/>
					<COMMAND LEAF_NODE_VALUE="1" ID="DE" remarks="ENCENDER DISPLAY"/>
				</COMMAND_ON_SNMP_SET>
				<COMMAND_ON_SNMP_SET OID="1.3.6.1.4.1.46536.2.6.1.5.0">
					<COMMAND LEAF_NODE_VALUE="0" ID="SA" remarks="APAGAR TERMOSTATO"/>
					<COMMAND LEAF_NODE_VALUE="1" ID="SE" remarks="ENCENDER TERMOSTATO"/>
				</COMMAND_ON_SNMP_SET>
			</CARD>
			<CARD ID="ER1">
				<QUERY ID="L?" PERIOD="1" remarks="LIGHT_SENSOR_LEVEL"/>
				<SCHEDULED-QUERY ID="E?" AT_SCHEDULED_TIME="03:01" ELEMENT_IDS="wrongLeds, wrongModules"/>
				<SW-QUERY ELEMENT_IDS="usb" remarks="ERRORS_CARD_USB_COMMPORT"/>
				<SW-NOTIFICATION ELEMENT="masterUsbErrorsNotification" CONDITION="STRING_VALUE_ERROR_CONDITION"/>
				<SW-NOTIFICATION ELEMENT="masterLedNotification" CONDITION="BOUND" UPPER_BOUND="10"/>
				<SW-NOTIFICATION ELEMENT="masterModuleNotification" CONDITION="BOUND" UPPER_BOUND="0"/>
				<SW-NOTIFICATION ELEMENT="masterLightSensorNotification" CONDITION="BOUND" LOWER_BOUND="1"/>
			</CARD>
		</FACE>
		<AGENT_CHECKERS>
			<CHECKER OID="1.3.6.1.4.1.46536.2.5.0" remarks="CHECK FOR OVERALL DISPLAY STATUS" OK="Ok" KO="Error">
				<CHECK OID="1.3.6.1.4.1.46536.2.6.1.1.0" remarks="MASTER ALARMS CARD USB COMMPORT"/>
				<CHECK OID="1.3.6.1.4.1.46536.2.6.1.4.0" remarks="MASTER INTERNAL TEMPERATURE"/>
				<CHECK OID="1.3.6.1.4.1.46536.2.6.2.1.0" remarks="MASTER ERRORS_CARD USB COMMPORT"/>
				<CHECK OID="1.3.6.1.4.1.46536.2.6.2.2.0" remarks="MASTER LIGHT SENSOR"/>
				<CHECK OID="1.3.6.1.4.1.46536.2.7.1.1.0" remarks="SLAVE ALARMS CARD USB COMMPORT"/>
				<CHECK OID="1.3.6.1.4.1.46536.2.7.1.4.0" remarks="SLAVE INTERNAL TEMPERATURE"/>
				<CHECK OID="1.3.6.1.4.1.46536.2.7.2.1.0" remarks="SLAVE ERRORS_CARD USB COMMPORT"/>
				<CHECK OID="1.3.6.1.4.1.46536.2.7.2.2.0" remarks="SLAVE LIGHT SENSOR"/>
			</CHECKER>
		</AGENT_CHECKERS>
		<NOTIFICATIONS>
			<PARAMETER ID="ADMINISTRATOR_IP_ADDRESS" VALUE="192.168.1.5"/>
			<PARAMETER ID="ADMINISTRATOR_PORT" VALUE="162"/>
			<PARAMETER ID="ADMINISTRATOR_READ_ONLY_COMMUNITY" VALUE="public"/>
			<PARAMETER ID="ADMINISTRATOR_READ_WRITE_COMMUNITY" VALUE="textel"/>
		</NOTIFICATIONS>
		<JFRAME>
			<JFRAME_COMMANDS>
				<COMMAND_BUTTON ID="cmdMasterDisplayOn" CARD_ID="AL1" FACE_ID="master" COMMAND="DE"/>
				<COMMAND_BUTTON ID="cmdMasterDisplayOff" CARD_ID="AL1" FACE_ID="master" COMMAND="DA"/>
				<COMMAND_BUTTON ID="cmdMasterTermostatoOn" CARD_ID="AL1" FACE_ID="master" COMMAND="SE"/>
				<COMMAND_BUTTON ID="cmdMasterTermostatoOff" CARD_ID="AL1" FACE_ID="master" COMMAND="SA"/>
				<COMMAND_BUTTON ID="cmdMasterScanLeds" CARD_ID="ER1" FACE_ID="master" COMMAND="E?"/>
				<COMMAND_BUTTON ID="cmdSlaveDisplayOn" CARD_ID="AL2" FACE_ID="slave" COMMAND="DE"/>
				<COMMAND_BUTTON ID="cmdSlaveDisplayOff" CARD_ID="AL2" FACE_ID="slave" COMMAND="DA"/>
				<COMMAND_BUTTON ID="cmdSlaveTermostatoOn" CARD_ID="AL2" FACE_ID="slave" COMMAND="SE"/>
				<COMMAND_BUTTON ID="cmdSlaveTermostatoOff" CARD_ID="AL2" FACE_ID="slave" COMMAND="SA"/>
				<COMMAND_BUTTON ID="cmdSlaveScanLeds" CARD_ID="ER2" FACE_ID="slave" COMMAND="E?"/>
			</JFRAME_COMMANDS>
			<JFRAME_UPDATERS>
				<UPDATER JFRAME="frameManager">
					<UPDATE COMPONENT_IN_JFRAME="labelMasterLightSensor" COMPONENT_TYPE="JLabel" SUFIX="%"/>
					<UPDATE COMPONENT_IN_JFRAME="labelSlaveLightSensor" COMPONENT_TYPE="JLabel" SUFIX="%"/>
					<UPDATE COMPONENT_IN_JFRAME="labelMasterDoor" COMPONENT_TYPE="JLabel" OPTION1="A=abierta" OPTION2="C=cerrada"/>
					<UPDATE COMPONENT_IN_JFRAME="labelSlaveDoor" COMPONENT_TYPE="JLabel" OPTION1="A=abierta" OPTION2="C=cerrada"/>
					<UPDATE COMPONENT_IN_JFRAME="labelMasterTemperature" COMPONENT_TYPE="JLabel" SUFIX="°C"/>
					<UPDATE COMPONENT_IN_JFRAME="labelSlaveTemperature" COMPONENT_TYPE="JLabel" SUFIX="°C"/>
					<UPDATE COMPONENT_IN_JFRAME="labelMasterErrorLeds" COMPONENT_TYPE="JLabel" SUFIX="leds"/>
					<UPDATE COMPONENT_IN_JFRAME="labelSlaveErrorLeds" COMPONENT_TYPE="JLabel" SUFIX="leds"/>
					<UPDATE COMPONENT_IN_JFRAME="labelMasterErrorModules" COMPONENT_TYPE="JLabel" SUFIX="fila(s)"/>
					<UPDATE COMPONENT_IN_JFRAME="labelSlaveErrorModules" COMPONENT_TYPE="JLabel" SUFIX="fila(s)"/>
					<UPDATE COMPONENT_IN_JFRAME="masterDisplayButton" COMPONENT_TYPE="ButtonPair"/>
					<UPDATE COMPONENT_IN_JFRAME="slaveDisplayButton" COMPONENT_TYPE="ButtonPair"/>
					<UPDATE COMPONENT_IN_JFRAME="masterTermostatoButton" COMPONENT_TYPE="ButtonPair"/>
					<UPDATE COMPONENT_IN_JFRAME="slaveTermostatoButton" COMPONENT_TYPE="ButtonPair"/>
				</UPDATER>
			</JFRAME_UPDATERS>
		</JFRAME>
		<VEILS>
			<VEIL POS_X="50" POS_Y="50" WIDTH="192" HEIGHT="48" MINIMUM_BRIGHTNESS="40" BRIGHTNESS_PROFILE="SUN"/>
		</VEILS>
	</PANEL>
	<MISC>
		<BEHAVIOUR_ON_USB_ERROR ATTEMPTS_TO_RESTART_APP_BEFORE_RESTART_PC="3" MAX_ATTEMPTS_TO_RESTART_PC_BY_CARD="3"/>
		<SWITCH_OFF_DISPLAY CARD_IDS="AL1,AL2" COMMAND="DA"/>
		<QUERY_FOR_CARD_ID ID="Q?"/>
		<BRIGHTNESS_PROFILES>
			<PROFILE ID="SUN" X_POS_REF="50" Y_POS_REF="90"/>
			<PROFILE ID="SHADE" X_POS_REF="50" Y_POS_REF="70"/>
			<PROFILE ID="INDOOR" X_POS_REF="50" Y_POS_REF="50"/>
		</BRIGHTNESS_PROFILES>
	</MISC>
</CONFIG>
