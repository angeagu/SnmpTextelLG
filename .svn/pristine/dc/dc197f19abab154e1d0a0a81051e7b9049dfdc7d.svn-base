<CONFIG>
	<PANEL COLUMNS="32" ROWS="16" MODULE_WIDTH="1" MODULE_HEIGHT="1">
		<FACE ID="master">
			<CARD ID="AL1" PROTOCOL="TEXTEL" QUERY_FOR_CARD_ID="Q?">
				<ON-START-COMMAND ID="DE" DELAY="15"/>
				<QUERY ID="P?" CARD_RESPONSE_FORMAT="P*" PERIOD="5" ON_START="1" OIDS="1.3.6.1.4.1.46536.2.6.1.3.0" remarks="DOOR OPEN/CLOSED" LOG="0"/>
				<QUERY ID="D?" CARD_RESPONSE_FORMAT="D#" PERIOD="5" ON_START="1" OIDS="1.3.6.1.4.1.46536.2.6.1.2.0" remarks="DISPLAY ON/OFF" LOG="0"/>
				<QUERY ID="L?" CARD_RESPONSE_FORMAT="L###" PERIOD="1" ON_START="-1" OIDS="1.3.6.1.4.1.46536.2.6.2.2.0" remarks="LIGHT_SENSOR_LEVEL" LOG="0"/>
				<QUERY ID="T?" CARD_RESPONSE_FORMAT="T.###" PERIOD="5" ON_START="1" OIDS="1.3.6.1.4.1.46536.2.6.1.4.0" remarks="INTERNAL_TEMPERATURE" LOG="0"/>
				<QUERY ID="S?" CARD_RESPONSE_FORMAT="S#" PERIOD="5" ON_START="1" OIDS="1.3.6.1.4.1.46536.2.6.1.5.0" remarks="THERMOSTAT ON/OFF" LOG="0"/>
				<QUERY ID="V?" CARD_RESPONSE_FORMAT="V1#V2#" PERIOD="5" ON_START="2" OIDS="1.3.6.1.4.1.46536.2.6.1.6,1.3.6.1.4.1.46536.2.6.1.7" remarks="FANS ON/OFF" LOG="0"/>
				<SW-QUERY ELEMENT_IDS="usb" OIDS="1.3.6.1.4.1.46536.2.6.2.1.0" remarks="ERRORS_CARD_USB_COMMPORT"/>
				<HW-NOTIFICATION NOTIFICATION_FORMAT="P." OID="1.3.6.1.4.1.46536.2.6.3.4.0" MESSAGE="Door Open"/>
				<HW-NOTIFICATION NOTIFICATION_FORMAT="G##" OID="1.3.6.1.4.1.46536.2.6.3.9.0" MESSAGE="Hit received with a G-Force of "/>
				<HW-NOTIFICATION NOTIFICATION_FORMAT="D." OID="1.3.6.1.4.1.46536.2.6.3.1.0" MESSAGE="Display Off"/>
				<SW-NOTIFICATION OID="1.3.6.1.4.1.46536.2.6.1.4.0" LOWER_BOUND="0" UPPER_BOUND="35" NOTIFICATION_OID="1.3.6.1.4.1.46536.2.6.3.5.0" ELEMENT="masterTemperatureNotification" CONDITION="BOUNDS"/>
				<SW-NOTIFICATION OID="1.3.6.1.4.1.46536.2.6.2.1.0" NOTIFICATION_OID="1.3.6.1.4.1.46536.2.6.3.3.0" ELEMENT="masterUsbErrorsNotification" STRING_VALUE_ERROR_CONDITION="" CONDITION="STRING_VALUE_ERROR_CONDITION"/>
				<SW-NOTIFICATION OID="1.3.6.1.4.1.46536.2.6.1.5.0" NOTIFICATION_OID="1.3.6.1.4.1.46536.2.6.3.6.0" ELEMENT="masterFansNotification" CONDITION="VALUE_MUST_MATCH_TO" VALUE_MUST_MATCH_TO="1.3.6.1.4.1.46536.2.6.1.6,1.3.6.1.4.1.46536.2.6.1.7" RETRIES="3"/>
				<SW-NOTIFICATION OID="1.3.6.1.4.1.46536.2.6.2.2.0" LOWER_BOUND="1" UPPER_BOUND="255" NOTIFICATION_OID="1.3.6.1.4.1.46536.2.6.3.10.0" ELEMENT="masterLightSensorNotification" CONDITION="BOUNDS"/>
				<COMMAND_ON_SNMP_SET OID="1.3.6.1.4.1.46536.2.6.1.2.0">
					<COMMAND LEAF_NODE_VALUE="0" ID="DA" remarks="APAGAR DISPLAY"/>
					<COMMAND LEAF_NODE_VALUE="1" ID="DE" remarks="ENCENDER DISPLAY"/>
				</COMMAND_ON_SNMP_SET>
				<COMMAND_ON_SNMP_SET OID="1.3.6.1.4.1.46536.2.6.1.5.0">
					<COMMAND LEAF_NODE_VALUE="0" ID="SA" remarks="APAGAR TERMOSTATO"/>
					<COMMAND LEAF_NODE_VALUE="1" ID="SE" remarks="ENCENDER TERMOSTATO"/>
				</COMMAND_ON_SNMP_SET>
			</CARD>
			<CARD ID="01" PROTOCOL="LG" QUERY_FOR_CARD_ID="ka ID FF">
				<QUERY ID="ka ID FF" CARD_RESPONSE_FORMAT="a_##_**##" PERIOD="5" ON_START="1" OIDS="1.3.6.1.4.1.46536.2.6.4.2.0" remarks="POWER ON/OFF" LOG="0"/>
				<SW-QUERY ELEMENT_IDS="usb" OIDS="1.3.6.1.4.1.46536.2.6.4.1.0" remarks="ERRORS_CARD_USB_COMMPORT"/>
				<SW-NOTIFICATION OID="1.3.6.1.4.1.46536.2.6.4.1.0" NOTIFICATION_OID="1.3.6.1.4.1.46536.2.6.3.11.0" ELEMENT="masterMonitorUsbErrorsNotification" STRING_VALUE_ERROR_CONDITION="" CONDITION="STRING_VALUE_ERROR_CONDITION"/>
				<ON-START-COMMAND ID="ka ID 01" DELAY="16"/>
				<COMMAND_ON_SNMP_SET OID="1.3.6.1.4.1.46536.2.6.4.2.0">
					<COMMAND LEAF_NODE_VALUE="00" ID="ka ID 00" remarks="APAGAR DISPLAY" CARD_RESPONSE_FORMAT="a_##_**##"/>
					<COMMAND LEAF_NODE_VALUE="01" ID="ka ID 01" remarks="ENCENDER DISPLAY" CARD_RESPONSE_FORMAT="a_##_**##"/>
				</COMMAND_ON_SNMP_SET>
			</CARD>
		</FACE>
		<AGENT_CHECKERS>
			<CHECKER OID="1.3.6.1.4.1.46536.2.5.0" remarks="CHECK FOR OVERALL DISPLAY STATUS" OK="Ok" KO="Error">
				<CHECK OID="1.3.6.1.4.1.46536.2.6.1.1.0" remarks="MASTER ALARMS CARD USB COMMPORT"/>
				<CHECK OID="1.3.6.1.4.1.46536.2.6.1.4.0" remarks="MASTER INTERNAL TEMPERATURE"/>
				<CHECK OID="1.3.6.1.4.1.46536.2.6.2.1.0" remarks="MASTER ERRORS_CARD USB COMMPORT"/>
				<CHECK OID="1.3.6.1.4.1.46536.2.6.2.2.0" remarks="MASTER LIGHT SENSOR"/>
			</CHECKER>
		</AGENT_CHECKERS>
		<NOTIFICATIONS>
			<PARAMETER ID="ADMINISTRATOR_IP_ADDRESS" VALUE="192.168.1.225"/>
			<PARAMETER ID="ADMINISTRATOR_PORT" VALUE="162"/>
			<PARAMETER ID="ADMINISTRATOR_READ_ONLY_COMMUNITY" VALUE="public"/>
			<PARAMETER ID="ADMINISTRATOR_READ_WRITE_COMMUNITY" VALUE="textel"/>
		</NOTIFICATIONS>
		<JFRAME>
			<JFRAME_COMMANDS>
				<COMMAND_BUTTON ID="cmdMasterTermostatoOn" CARD_ID="AL1" FACE_ID="master" COMMAND="SE"/>
				<COMMAND_BUTTON ID="cmdMasterTermostatoOff" CARD_ID="AL1" FACE_ID="master" COMMAND="SA"/>
				<COMMAND_BUTTON ID="cmdMasterDisplayOn" CARD_ID="01" FACE_ID="master" COMMAND="ka ID 01"/>
				<COMMAND_BUTTON ID="cmdMasterDisplayOff" CARD_ID="01" FACE_ID="master" COMMAND="ka ID 00"/>
			</JFRAME_COMMANDS>
			<JFRAME_UPDATERS>
				<UPDATER JFRAME="frameManager">
					<UPDATE OID="1.3.6.1.4.1.46536.2.6.2.2.0" SUFIX="%" FACTOR="0.390625" COMPONENT_IN_JFRAME="labelMasterLightSensor" DATA_TYPE="Veil" COMPONENT_TYPE="JLabel" OPTION1="" OPTION2=""/>
					<UPDATE OID="1.3.6.1.4.1.46536.2.6.1.3.0" SUFIX="" FACTOR="" COMPONENT_IN_JFRAME="labelMasterDoor" DATA_TYPE="String" COMPONENT_TYPE="JLabel" OPTION1="A=abierta" OPTION2="C=cerrada"/>
					<UPDATE OID="1.3.6.1.4.1.46536.2.6.1.4.0" SUFIX="Â°C" FACTOR="1" COMPONENT_IN_JFRAME="labelMasterTemperature" DATA_TYPE="Integer" COMPONENT_TYPE="JLabel" OPTION1="" OPTION2=""/>
					<UPDATE OID="1.3.6.1.4.1.46536.2.6.1.5.0" SUFIX="" FACTOR="1" COMPONENT_IN_JFRAME="masterTermostatoButton" DATA_TYPE="Integer" COMPONENT_TYPE="ButtonPair" OPTION1="" OPTION2=""/>
					<UPDATE OID="1.3.6.1.4.1.46536.2.6.4.2.0" SUFIX="" FACTOR="1" COMPONENT_IN_JFRAME="masterDisplayButton" DATA_TYPE="Integer" COMPONENT_TYPE="ButtonPair" OPTION1="" OPTION2=""/>
				</UPDATER>
			</JFRAME_UPDATERS>
		</JFRAME>
		<VEILS>
			<VEIL POS_X="100" POS_Y="100" WIDTH="320" HEIGHT="64" MINIMUM_BRIGHTNESS="40" CONVERSION_FACTOR="0.390625" OIDS="1.3.6.1.4.1.46536.2.6.2.2.0" EXTERNAL_VEIL_CARD="master-01" EXTERNAL_VEIL_CMD="kh ID VALUE" EXTERNAL_VEIL_RESPONSE="h_##_****"/>
		</VEILS>
		<DIRECT_TEXT>
			<PARAMETER ID="FONT_NAME" OID="1.3.6.1.4.1.46536.4.1.0" VALUE="Arial"/>
			<PARAMETER ID="FONT_STYLE" OID="1.3.6.1.4.1.46536.4.2.0" VALUE="1"/>
			<PARAMETER ID="FONT_COLOR" OID="1.3.6.1.4.1.46536.4.3.0" VALUE="255, 255, 255"/>
			<PARAMETER ID="TEXT_HEADER" OID="1.3.6.1.4.1.46536.4.4.0" VALUE=""/>
			<PARAMETER ID="TEXT_MESSAGE" OID="1.3.6.1.4.1.46536.4.5.0" VALUE=""/>
			<PARAMETER ID="LIFESPAN" OID="1.3.6.1.4.1.46536.4.6.0" VALUE="1"/>
			<SW_COMMAND_ON_SNMP_SET DATA="DIRECT_TEXT,0,PARAMETER,ID,FONT_NAME,VALUE" DESTINATION="XML" OID="1.3.6.1.4.1.46536.4.1.0"/>
			<SW_COMMAND_ON_SNMP_SET DATA="DIRECT_TEXT,0,PARAMETER,ID,FONT_STYLE,VALUE" DESTINATION="XML" OID="1.3.6.1.4.1.46536.4.2.0"/>
			<SW_COMMAND_ON_SNMP_SET DATA="DIRECT_TEXT,0,PARAMETER,ID,FONT_COLOR,VALUE" DESTINATION="XML" OID="1.3.6.1.4.1.46536.4.3.0"/>
			<SW_COMMAND_ON_SNMP_SET DATA="" DESTINATION="MEMORY" OID="1.3.6.1.4.1.46536.4.4.0"/>
			<SW_COMMAND_ON_SNMP_SET DATA="" DESTINATION="MEMORY" OID="1.3.6.1.4.1.46536.4.5.0"/>
			<SW_COMMAND_ON_SNMP_SET DATA="DIRECT_TEXT,0,PARAMETER,ID,LIFESPAN,VALUE" DESTINATION="XML" OID="1.3.6.1.4.1.46536.4.6.0"/>
		</DIRECT_TEXT>
	</PANEL>
	<MISC>
		<BEHAVIOUR_ON_USB_ERROR SECONDS_TO_RESTART_APP="30" ATTEMPTS_TO_RESTART_APP_BEFORE_RESTART_PC="3" MAX_ATTEMPTS_TO_RESTART_PC_BY_CARD="3" DRIVE="D"/>
		<SWITCH_OFF_DISPLAY CARD_IDS="01" COMMAND="ka ID 00"/>
		<BRIGHTNESS_PROFILES>
			<PROFILE ID="SUN" X_POS_REF="50" Y_POS_REF="90"/>
			<PROFILE ID="SHADE" X_POS_REF="50" Y_POS_REF="70"/>
			<PROFILE ID="INDOOR" X_POS_REF="50" Y_POS_REF="50"/>
		</BRIGHTNESS_PROFILES>
	</MISC>
</CONFIG>
